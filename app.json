{
  "expo": {
    "name": "Just For Today",
    "slug": "just-for-today",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "justfortoday",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.gurumedia.JustForToday",
      "infoPlist": {
        "NSHealthShareUsageDescription": "Just For Today needs access to your step count data to track your daily activity.",
        "NSHealthUpdateUsageDescription": "Just For Today needs permission to update your health data.",
        "ITSAppUsesNonExemptEncryption": false,
        "NSUbiquitousContainers": {
          "iCloud.com.gurumedia.JustForToday": {}
        }
      },
      "entitlements": {
        "com.apple.developer.healthkit": true,
        "com.apple.developer.healthkit.access": []
      },
      "usesIcloudStorage": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.gurumedia.JustForToday",
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "android.permission.READ_CONTACTS",
        "android.permission.WRITE_CONTACTS"
      ],
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "@oleg_svetlichnyi/expo-icloud-storage",
        {
          "icloudContainerIdentifier": "iCloud.com.gurumedia.JustForToday"
        }
      ],
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow Just For Today to protect your private entries with Face ID."
        }
      ],
      "expo-font",
      "expo-sqlite",
      "expo-web-browser",
      [
        "expo-contacts",
        {
          "contactsPermission": "Just For Today needs access to your contacts so you can save trusted people to call during difficult moments."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "8402ad68-7cda-4c74-ae22-041adc48e444"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/8402ad68-7cda-4c74-ae22-041adc48e444"
    }
  }
}
